{
    "name": "vanrijs/twikey-api-php",
    "description": "Php client to interface with the Twikey api",
    "type": "library",
    "keywords": [
        "payments",
        "subscriptions",
        "twikey-php-client",
        "mollie",
        "payment",
        "service",
        "ideal",
        "creditcard",
        "mistercash",
        "bancontact",
        "sofort",
        "sofortbanking",
        "sepa",
        "banktransfer",
        "direct debit",
        "belfius",
        "refunds",
        "api",
        "payments",
        "gateway",
        "subscriptions",
        "recurring",
        "charges",
        "kbc",
        "cbc"
    ],
    "homepage": "https://api.twikey.com",
    "require": {
        "php": "^7.4 || ^8.0",
        "psr/http-client": "^1.0.1",
        "ext-json": "*",
        "ext-openssl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "guzzlehttp/guzzle": "^7.3"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Twikey\\Api\\": "src/"
        }
    },
    "scripts": {
        "test": "php vendor/phpunit/phpunit/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "authors": [
        {
            "name": "Koen Serry",
            "email": "482723+koen-serry@users.noreply.github.com"
        }
    ]
}
